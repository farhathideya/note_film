<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".creeCompte">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="507dp"
        android:layout_height="997dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backgroundloginflou" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="351dp"
        android:layout_height="551dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/backgroundlogin1arrondie"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.75"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="535dp"
            android:layout_marginRight="20dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.75"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="40dp"
                android:layout_marginBottom="15dp"
                android:text="email"
                android:textColor="#CDFFFFFF"
                android:textSize="20dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginLeft="60dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="40dp"
                    android:layout_height="40dp"


                    app:srcCompat="@drawable/images" />

                <EditText
                    android:id="@+id/mail"
                    android:layout_width="210dp"
                    android:layout_height="40dp"
                    android:background="#5CFFFFFF"
                    android:text=""
                    android:textColor="#E6FFFFFF" />


            </LinearLayout>


            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="15dp"
                android:text="password"
                android:textColor="#CDFFFFFF"
                android:textSize="20dp"
                android:textStyle="bold" />


            <LinearLayout
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginLeft="60dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVie"
                    android:layout_width="40dp"
                    android:layout_height="40dp"

                    app:srcCompat="@drawable/imagespassw4or4d" />

                <EditText
                    android:id="@+id/TextPassword"
                    android:layout_width="210dp"
                    android:layout_height="40dp"
                    android:background="#5CFFFFFF"
                    android:inputType="textPassword"
                    android:textColor="#E6FFFFFF" />


            </LinearLayout>

            <TextView
                android:id="@+id/confirmerpas"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="15dp"
                android:text="confirm password"
                android:textColor="#CDFFFFFF"
                android:textSize="20dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginLeft="60dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVie1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"

                    app:srcCompat="@drawable/imagespassw4or4d" />

                <EditText
                    android:id="@+id/confrmerPassword"
                    android:layout_width="210dp"
                    android:layout_height="40dp"
                    android:background="#5CFFFFFF"
                    android:inputType="textPassword"
                    android:textColor="#E6FFFFFF" />


            </LinearLayout>


            <Button
                android:id="@+id/login"
                android:layout_width="100dp"
                android:layout_height="55dp"
                android:layout_marginLeft="129dp"
                android:layout_marginTop="35dp"
                android:background="@drawable/arrondie"
                android:fontFamily="serif"
                android:text="login"
                android:textAlignment="center"
                android:textStyle="bold" />


            <Button
                android:id="@+id/retur"
                android:layout_width="100dp"
                android:layout_height="55dp"
                android:layout_marginLeft="129dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/arrondie"
                android:fontFamily="serif"
                android:text="return"
                android:textAlignment="center"
                android:textStyle="bold" />


        </LinearLayout>


    </LinearLayout>


    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="106dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.071"
        app:srcCompat="@drawable/imagelogoemail"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.995"
        app:srcCompat="@drawable/logonewhf"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>


