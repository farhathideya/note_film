<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".newlogin">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="558dp"
        android:layout_height="1067dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backgroundloginflou" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="363dp"
        android:layout_height="553dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/backgroundlogin1arrondie"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.73"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="315dp"
            android:layout_height="500dp"
            android:layout_marginRight="20dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.75"
            tools:ignore="MissingConstraints">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="60dp"
                android:layout_marginBottom="15dp"
                android:text="email"

                android:textColor="#CDFFFFFF"
                android:textSize="20dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginLeft="60dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    app:srcCompat="@drawable/images" />

                <EditText
                    android:id="@+id/mail"
                    android:layout_width="190dp"
                    android:layout_height="40dp"
                    android:background="#5CFFFFFF"
                    android:hint=" Email"
                    android:text=""
                    android:textColor="#D2000000" />


            </LinearLayout>


            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="40dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="15dp"
                android:text="password"
                android:textColor="#CDFFFFFF"
                android:textSize="20dp"
                android:textStyle="bold" />


            <LinearLayout
                android:layout_width="250dp"
                android:layout_height="40dp"
                android:layout_marginLeft="60dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageVie"
                    android:layout_width="40dp"
                    android:layout_height="40dp"

                    app:srcCompat="@drawable/imagespassw4or4d" />

                <EditText
                    android:id="@+id/Password"
                    android:layout_width="190dp"
                    android:layout_height="40dp"
                    android:background="#5CFFFFFF"
                    android:hint=" Password"
                    android:inputType="textPassword" />
            </LinearLayout>

            <TextView
                android:id="@+id/modePasseOublier"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="25dp"
                android:text="forget password?"
                android:textColor="#CDFFFFFF"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/cree"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="5dp"
                android:text="create an account?"
                android:textColor="#CDFFFFFF"
                android:textStyle="bold" />

            <Button
                android:id="@+id/login1"
                android:layout_width="100dp"
                android:layout_height="55dp"
                android:layout_marginLeft="135dp"
                android:layout_marginTop="70dp"
                android:background="@drawable/arrondie"
                android:fontFamily="serif"
                android:text="login"
                android:textStyle="bold" />


        </LinearLayout>


    </LinearLayout>


    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="106dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.059"
        app:srcCompat="@drawable/imagelogoemail"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="73dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.952"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        app:srcCompat="@drawable/logonewhf"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>